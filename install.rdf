    <!-- Front End MetaData -->
    <em:name>Only Icons: Bookmark Shortcuts</em:name>
    <em:description>Adds a context menu entry to each entry of the bookmarks toolbar for hiding the bookmark labels.</em:description>
    <em:creator>Christian Schmitz &lt;tynn.dev@gmail.com&gt;</em:creator>

    <!-- L10N -->
    <em:localized>
      <Description>
        <em:locale>en</em:locale>
        <em:name>Only Icons: Bookmark Shortcuts</em:name>
        <em:description>Adds a context menu entry to each entry of the bookmarks toolbar for hiding the bookmark labels.</em:description>
        <em:creator>Christian Schmitz &lt;tynn.dev@gmail.com&gt;</em:creator>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>de</em:locale>
        <em:name>Nur Icons: Lesezeichen-Shortcuts</em:name>
        <em:description>Fügt den Elementen der Lesezeichen-Symbolleiste einen Kontextmenüeintrag zum Verbergen des Lesezeichentitels hinzu.</em:description>
